{
  "name": "personal-finance",
  "version": "1.0.0",
  "description": "��#\u0000 \u0000p\u0000e\u0000r\u0000s\u0000o\u0000n\u0000a\u0000l\u0000-\u0000f\u0000i\u0000n\u0000a\u0000n\u0000c\u0000e\u0000\r\u0000 \u0000",
  "main": "index.js",
  "scripts": {
     "dev": "concurrently -n SERVER,CLIENT,PYTHON -c green,magenta,cyan \"npm:dev:server\" \"npm:dev:client\" \"npm:dev:market-data\"",
    "dev:server": "tsx watch server/main.ts",
    "dev:client": "npm run dev --prefix client",
    "dev:market-data": "bash -lc 'set -a && source .env && set +a && cd market-data && source .venv/bin/activate && python run.py'",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.json",
    "build:client": "npm run build --prefix client",
    "start": "node dist/main.js",
    "preview": "npm run preview --prefix client",
    "prisma:validate": "prisma validate --schema prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma --name init",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noumankhan20/personal-finance.git"
  },
  "keywords": [],
